<% provide(:title, 'All users') %>
<h1>All users</h1>
<div class="row">
  <div class="col-xs-6 col-md-8"></div>
  <div class="col-xs-6 col-md-4">
    <%= link_to "Add new Trainee", admin_signup_path, class: "edit-btn btn btn-primary" %>
  </div>
</div>
<div class="row">
  <ul class="users">
    <%= will_paginate %>
      <% @users.each do |user| %>
        <%= render user %>
      <% end %>
    <%= will_paginate %>
  </ul>
</div>

